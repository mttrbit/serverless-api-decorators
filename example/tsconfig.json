{
  "compilerOptions": {
   "target": "es5",
    "module": "commonjs",
    "alwaysStrict": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "rootDir": "./",
    "outDir": "lib",
    "preserveConstEnums": true,
    "removeComments": true
  },
  "exclude": [
    "api/user",
    "lib",
    "node_modules"

  ],
  "compileOnSave": true
}
